{
    "name": "api-analytics/php",
    "description": "API Analytics middleware for native PHP applications.",
    "keywords": ["analytics", "api", "monitoring", "php", "middleware"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Draper",
            "email": "tomjdraper1@gmail.com"
        }
    ],
    "homepage": "https://github.com/tom-draper/api-analytics",
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "api-analytics/core": "^1.0 || dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "ApiAnalytics\\PHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiAnalytics\\PHP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
